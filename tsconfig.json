{
  "extends": "./tsconfig.build.json",
  "compilerOptions": {
    "importHelpers": true,
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@arjit95/routi-*": ["packages/*/src"]
    }
  }
}
